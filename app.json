{
  "expo": {
    "name": "mobileApp",
    "slug": "mobileApp",
    "android": {
      "package": "com.beyonception.rentapp"
  },
  "ios": {
    "bundleIdentifier": "com.beyonception.rentapp",
    "supportsTablet": true
  },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
